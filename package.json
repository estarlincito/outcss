{
  "name": "outcss",
  "version": "1.0.1",
  "description": "A CSS-out library for generating static CSS files from JS",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc && npm run css-build",
    "css-build": "postcss src/styles.css --output dist/styles.css"
  },
  "files": [
    "dist",
    "src",
    "dist/styles.css"
  ],
  "keywords": [
    "css",
    "postcss",
    "outcss"
  ],
  "author": "Estarlincito",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "typescript": "^5.7.2",
    "uuid": "^11.0.4"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estarlincito/outcss.git"
  },
  "bugs": {
    "url": "https://github.com/estarlincito/outcss/issues"
  },
  "homepage": "https://github.com/estarlincito/outcss#readme"
}
